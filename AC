	end;
end;
game:GetService('StarterGui'):SetCore("SendNotification", {
	Title = 'AC Hub';
	Text = 'Would You Like To Enable Chatspy';
	Icon = "rbxassetid://10716387808";
	Duration = 3.4028235e+38;
	Callback = b;
	Button1 = 'Yes!';
	Button2 = 'No!';
});


