			for i = 1, 100 do
				wait();
				workspace:FindFirstChild('MAP'):FindFirstChild('SPECIAL_BRIDGE').Size = workspace:FindFirstChild('MAP'):FindFirstChild('SPECIAL_BRIDGE').Size + Vector3.new(1, 0, 0);
			end;
			wait(5);
