function NovaNoti(Title, Text, Icon, Duration)
	game.StarterGui:SetCore("SendNotification", {
		Title = Title;
		Text = Text;
		Duration = 10;
	})
	local cor = coroutine.wrap(function()
		local sound = Instance.new("Sound", game.CoreGui)
		sound.SoundId = "rbxassetid://3442983711"
		sound:Play()
		wait(4)
		sound:Destroy()
	end)
	cor()
end
