	wait(.1)
	fireclickdetector(game.Workspace.Ignored.Shop[toolname].ClickDetector)
	wait(.1)
	fireclickdetector(game.Workspace.Ignored.Shop[toolname].ClickDetector)
	wait(.1)
	fireclickdetector(game.Workspace.Ignored.Shop[toolname].ClickDetector)
	wait(.1)
	fireclickdetector(game.Workspace.Ignored.Shop[toolname].ClickDetector)
	wait(.1)
	fireclickdetector(game.Workspace.Ignored.Shop[toolname].ClickDetector)
