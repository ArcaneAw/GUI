	game.Players.localPlayer.CameraMaxZoomDistance = 1000
	game.Players.LocalPlayer.CameraMinZoomDistance = 0
end)
MainPage.AddButton("No Fog", function()
    game:GetService("Lighting").FogEnd = 9e9
    game:GetService("Lighting").FogStart = 9e9
end)
MainPage.AddButton("No Trail", function()
     game.Players.LocalPlayer.Character.Humanoid:WaitForChild("TrailEffects"):Destroy()
end)
MainPage.AddButton("No Hats", function()
    	for i,v in next, game.Players.LocalPlayer.Character:GetDescendants() do
		if v:IsA("Accessory") then
			for i,p in next, v:GetDescendants() do
				if p:IsA("Weld") then
					p:Destroy()
				end
			end
		end
	end
end)
MainPage.AddButton("No Arms", function()
    
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("BasePart") and
				v.Name == "RightUpperArm" or
				v.Name == "LeftUpperArm" then
				v:Destroy()
			end
		end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("BasePart") and
				v.Name == "Right Arm" or
				v.Name == "Left Arm" then
				v:Destroy()
			end
		end
end)
MainPage.AddButton("No legs", function()
    	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("BasePart") and
				v.Name == "RightUpperLeg" or
				v.Name == "LeftUpperLeg" then
				v:Destroy()
				end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("BasePart") and
				v.Name == "Right Leg" or
				v.Name == "Left Leg" then
				v:Destroy()
			end
		end
	end
end)
